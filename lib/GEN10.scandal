// Csound's GEN10 subroutine.
lambda GEN10 = array args -> {
	int length = floor(args[0])
	array table = new(length)
	int i = 0
	int j = 1
	while i < length {
		while j < size(args) {
			table[i] = table[i] + args[j] * cos(j * 2 * pi * i / length - pi / 2)
			j = j + 1
		}
		j = 1
		i = i + 1
	}
	return table
}