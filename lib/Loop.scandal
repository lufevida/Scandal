// Loop an array.
lambda loop = array x -> int start -> int end -> int count -> {
	int length = end - start + 1
	int samples = length * count
	array buffer = new(samples)
	int i = 0
	int j = 0
	while i < samples {
		buffer[i] = x[j % length + start]
		i = i + 1
		j = j + 1
	}
	return buffer
}

